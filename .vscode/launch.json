{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Compiled Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/js/cbl0001.js",
      "env": {
        "ACCTREC": "js/data",
        "PRTLINE": "js/prtline"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Written Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/js/test-decode/cbl0001.js",
      "env": {
        "ACCTREC": "js/test-decode/data",
        "PRTLINE": "js/test-decode/prtline"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Simple Read Write",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/examples/simple-read-write/rw.js",
      "env": {
        "ACCTREC": "examples/simple-read-write/data",
        "PRTLINE": "examples/simple-read-write/prtline"
      }
    },
  ]
}